<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
 

    <record id="action_report_saleorder" model="ir.actions.report">
        <field name="name">Jewellery / Order</field>
        <field name="model">sales_detail.customers</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">sales_sample_order.report_jewellery</field>
        <field name="report_file">sales_sample_order.report_jewellery</field>
        <field name="print_report_name">(object.jewellery_type in ('draft', 'sent') and 'Quotation - %s' % (object.current_date)) or 'Order - %s' % (object.current_date)</field>
        <!-- <field name="binding_model_id" ref="model_sale_order"/> --> 
        <field name="binding_type">report</field>
    </record>

</odoo>
