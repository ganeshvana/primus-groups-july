<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="report_saleorder">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="doc">
            <t t-call="web.external_layout">
                <h2>Jewellery Details</h2>
                <table style="width:100%">
                  <tr>
                    <th>Date:</th>
                    <th>jewellery_type:</th> 
                    <th>Price:</th>
                  </tr>
                  <tr>
                    <td><t t-esc="doc.current_date"/></td>
                    <td><t t-esc="doc.jewellery_type"/></td>
                    <td><t t-esc="doc.Price"/></td>
                  </tr>
              </table>
            </t>
        </t>
    </t>
</template>

<template id="report_jewellery">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="doc">
            <t t-call="sales_sample_order.report_saleorder"/>
        </t>
    </t>
</template>

</odoo>